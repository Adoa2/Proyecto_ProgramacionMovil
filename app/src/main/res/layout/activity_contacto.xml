<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Contacto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#4E342E"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="125dp"
        tools:layout_editor_absoluteY="169dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/img1">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="215dp"
            android:layout_height="59dp"
            android:fontFamily="sans-serif-black"
            android:text="@string/contacto"
            android:textColor="@color/white"
            android:textSize="50sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="20dp" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="214dp"
            android:layout_height="56dp"
            android:fontFamily="sans-serif-black"
            android:text="Contacto"
            android:textColor="#4E342E"
            android:textSize="50sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="24dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="370dp"
            android:layout_height="40dp"
            android:background="#8B4513"
            android:fontFamily="sans-serif-black"
            android:text="@string/informaci_n_de_la_cafeter_a"
            android:textColor="@color/white"
            android:textSize="24sp"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="30dp"
            android:layout_marginStart="16dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="382dp"
            android:layout_height="36dp"
            android:background="#7f3c19"
            android:fontFamily="sans-serif-black"
            android:text="@string/redes_sociales_cafe123"
            android:textColor="@color/white"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="20dp"
            android:layout_marginStart="16dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="370dp"
            android:layout_height="60dp"
            android:background="#7f3c19"
            android:fontFamily="sans-serif-black"
            android:text="Horario: Lun-Vie 8:00 - 18:00, Sáb-Dom 9:00 - 17:00"
            android:textColor="@color/white"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="20dp"
            android:layout_marginStart="16dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="370dp"
            android:layout_height="65dp"
            android:background="#7f3c19"
            android:fontFamily="sans-serif-black"
            android:text="Dirección: Barrio El Centro Café 123, Ciudad de Siguatepeque"
            android:textColor="@color/white"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/textView5"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="20dp"
            android:layout_marginStart="16dp" />

        <LinearLayout
            android:layout_width="230dp"
            android:layout_height="285dp"
            android:background="@drawable/img3"
            android:orientation="vertical"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.45"
            app:layout_constraintHorizontal_bias="0.5"/>

        <LinearLayout
            android:layout_width="10dp"
            android:layout_height="236dp"
            android:background="#4E342E"
            android:orientation="vertical"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            app:layout_constraintStart_toEndOf="@+id/main_layout"
            android:layout_marginTop="20dp"
            android:layout_marginStart="16dp" />

        <LinearLayout
            android:layout_width="10dp"
            android:layout_height="236dp"
            android:background="#4E342E"
            android:orientation="vertical"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            app:layout_constraintEnd_toStartOf="@+id/main_layout"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="16dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
